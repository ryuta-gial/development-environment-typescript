{
    "name": "test",
    "version": "1.0.0",
    "main": "index.js",
    "author": "ryuta",
    "license": "MIT",
    "scripts": {
        "client": "webpack serve --config ./webpack.config.js --open /main --mode development",
        "server": "ts-node-dev --project ./server/tsconfig.json --respawn ./server/index.ts",
        "devClient": "webpack serve --config ./config/webpack.config.dev.js ",
        "build": "webpack --config ./webpack.config.js --mode development",
        "buildServer": "tsc --project ./server/tsconfig.json",
        "tryCode": "ts-node-dev --project ./src/tryCode/tsconfig.json --respawn ./src/tryCode/index.ts ",
        "tsc": "tsc",
        "lint": "eslint --cache --fix src && prettier --write src",
        "prettierLint": "npx prettier --write .",
        "dev": "concurrently \"yarn run devClient\" \"yarn run server\"",
        "start": "yarn run cbuild && yarn run server"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^5.14.0",
        "@typescript-eslint/parser": "^5.14.0",
        "clean-webpack-plugin": "^4.0.0",
        "concurrently": "^7.0.0",
        "css-loader": "^6.7.1",
        "eslint": "^8.11.0",
        "eslint-config-prettier": "^8.5.0",
        "hard-source-webpack-plugin": "^0.13.1",
        "html-loader": "^3.1.0",
        "html-webpack-plugin": "^5.5.0",
        "nodemon": "^2.0.15",
        "prettier": "^2.5.1",
        "style-loader": "^3.3.1",
        "ts-loader": "^9.2.8",
        "ts-node": "^10.7.0",
        "ts-node-dev": "^1.1.8",
        "typescript": "^4.6.2",
        "webpack": "^5.70.0",
        "webpack-cli": "^4.9.2",
        "webpack-dev-server": "^4.7.4",
        "webpack-merge": "^5.8.0"
    },
    "dependencies": {
        "@types/axios": "^0.14.0",
        "@types/cors": "^2.8.12",
        "@types/express": "^4.17.13",
        "@types/node": "^17.0.21",
        "axios": "^0.26.1",
        "cors": "^2.8.5",
        "dotenv": "^16.0.2",
        "express": "^4.17.3"
    }
}
